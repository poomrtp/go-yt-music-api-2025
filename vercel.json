{
  "version": 2,
  "builds": [
    {
      "src": "api/index.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "status": 204
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.go"
    }
  ],
  "functions": {
    "api/**": {
      "maxDuration": 20
    }
  }
}
